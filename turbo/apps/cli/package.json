{
  "name": "@uspark/cli",
  "version": "0.10.5",
  "private": true,
  "description": "CLI application",
  "type": "module",
  "bin": {
    "uspark": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "postbuild": "cp package.json dist/ && pnpm json -I -f dist/package.json -e 'delete this.private; delete this.scripts; delete this.devDependencies; delete this.dependencies[\"@uspark/core\"]; this.bin[\"uspark\"]=\"./index.js\"; this.files=[\".\"]'",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@uspark/core": "workspace:*",
    "@vercel/blob": "^1.1.1",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "js-sha256": "^0.11.1",
    "signal-timers": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@uspark/eslint-config": "workspace:*",
    "@uspark/typescript-config": "workspace:*",
    "eslint": "^9.34.0",
    "json": "^11.0.0",
    "msw": "^2.11.1",
    "tsup": "^8.5.0",
    "typescript": "5.9.2",
    "vitest": "^3.2.4",
    "yjs": "^13.6.21"
  }
}
